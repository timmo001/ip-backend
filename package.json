{
  "name": "ip-backend",
  "version": "1.0.0",
  "description": "ip backend",
  "private": true,
  "author": {
    "name": "Aidan Timson <Timmo>",
    "email": "contact@timmo.xyz",
    "url": "https://ip.timmo.dev"
  },
  "keywords": [
    "ip",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/timmo001/ip-backend"
  },
  "license": "MIT",
  "bin": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:eslint": "eslint --ext .ts --color \"{src,apps,libs,test}/**/*.ts\"",
    "package": "rimraf out && yarn build && node scripts/package.js",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --detectOpenHandles",
    "test:cov": "jest --forceExit --detectOpenHandles --reporters=default --reporters=jest-junit --coverage --coverageDirectory=output/coverage/jest --coverageReporters=cobertura",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles"
  },
  "dependencies": {
    "@nestjs/common": "8.0.6",
    "@nestjs/core": "8.0.6",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/platform-socket.io": "8.0.6",
    "@nestjs/swagger": "5.0.9",
    "@nestjs/typeorm": "8.0.2",
    "@nestjs/websockets": "8.0.6",
    "bcrypt": "5.0.1",
    "better-sqlite3": "7.4.3",
    "class-validator": "0.13.1",
    "cors": "2.8.5",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "mariadb": "2.5.4",
    "mysql": "2.18.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.37",
    "uuid": "8.3.2",
    "ws": "8.0.0",
    "yaml": "1.10.2"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.1",
    "@nestjs/schematics": "8.0.2",
    "@nestjs/testing": "8.0.6",
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "16.4.13",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "27.0.6",
    "jest-junit": "12.2.0",
    "pkg": "5.3.1",
    "prettier": "2.3.2",
    "supertest": "6.1.4",
    "ts-jest": "27.0.4",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "optionalDependencies": {
    "node-hide-console-window": "2.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*"
    ],
    "outputPath": "out",
    "scripts": "dist/main.js",
    "targets": [
      "host"
    ]
  }
}
